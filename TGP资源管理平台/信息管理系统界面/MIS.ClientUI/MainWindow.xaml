			</Grid.ColumnDefinitions>
			<Image Source="images/logo.png" Height="56" Margin="0,16"/>
			<Grid Grid.Column="1" Margin="0" Grid.ColumnSpan="2">
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="74*"/>
					<ColumnDefinition Width="175"/>
				</Grid.ColumnDefinitions>
				<StackPanel Margin="0" Orientation="Horizontal">
					<mis:NavigationUIItem  Style="{StaticResource DefaultNavigationUIItemStyle}"  Content="工作台" Height="88" Margin="0" HorizontalAlignment="Left" Width="87"/>
